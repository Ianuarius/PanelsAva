<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="250" d:DesignHeight="300"
             x:Class="PanelsAva.Views.HistoryPanelView">
  <StackPanel Spacing="6" Margin="8">
    <TextBlock Text="History" FontSize="16" />
    <ListBox>
      <ListBoxItem Content="Opened file" />
      <ListBoxItem Content="Brush stroke" />
      <ListBoxItem Content="Move layer" />
      <ListBoxItem Content="Fill" />
    </ListBox>
  </StackPanel>
</UserControl>